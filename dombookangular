                            $mdDialog.show(confirm).then(function () {

                                window.location = getpath() + "#/home";
                            },
                                function () {

                                    window.location = getpath() + "#/home";
                                });

                        }
                        else if (Ressponse["Status"] == "2") {
                            var confirm = $mdDialog.confirm()
                                          .title('Alert')
                                          .textContent(Ressponse["StatusDesc"])
                                          .ariaLabel('')
                                          .targetEvent(ev)
                                          .ok('OK')
