     function getpath() {
        try {
            var pathArray = window.location.href.split('/');
            var protocol = pathArray[0];
            var host = pathArray[2];
            var path = protocol + '//' + host + "/";
            if (host.indexOf(":") == -1) {
                path += pathArray[3] + "/";
            }
            return path;
        }
