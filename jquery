                lstrOngoing = lstrOngoing + obj.Ongoingstr;
            lstrReturn = lstrReturn + obj.Returnstr;

        });
        $("#OngoingRes").html(lstrOngoing);
        $("#ReturnRes").html(lstrReturn);
        SetDesignloc();
    }
    catch (e) {
        console.log('Error in Amountrangefilter() - ' + e.toString());
    }
}
